<header class="c-header">
  <img class="c-header__toggleopen" src="./assets/img/iconmenu-close.png" alt="toggle menu">
  <nav class="c-header__nav">
    <!-- HEADER PC -->
    <img class="c-header__toggleclose" src="./assets/img/iconmenu-open.png" alt="toggle menu">
    <ul>
      <% if (typeof linklists != 'undefined') { %>
        <% linklists.main && linklists.main.forEach(link => { %>
          <li class="c-header__navitem">
            <a <% if (link.current || link.active) { %>class="c-header__navlink is-active"<% } else { %> class="c-header__navlink"<% } %>  href="<%= link.url %>"><%= link.title %></a>
          </li>
        <% }) %>
      <% } %>
    </ul>
    </nav>
</header>
